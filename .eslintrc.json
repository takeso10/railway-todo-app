{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals":true,
        "node":true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jest",
        "prettier"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "no-unused-vars": "off"
    },
    "settings":{
        "react":{
            "version":"detect"
        }

    }
}
